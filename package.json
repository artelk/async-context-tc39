{
  "name": "async-context-tc39",
  "version": "1.0.5",
  "description": "AsyncContext (implementation for TC39)",
  "type": "module",
  "scripts": {
    "compile": "rm -rf dist && tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "compile:esm": "tsc -p tsconfig.esm.json",
    "compile:cjs": "tsc -p tsconfig.cjs.json && echo '{\"type\":\"commonjs\"}' > dist/cjs/package.json",
    "lint": "eslint .",
    "test": "vitest run"
  },
  "source": "./src/index.ts",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./dist/cjs/*.d.ts",
        "default": "./dist/cjs/*.js"
      },
      "default": {
        "types": "./dist/*.d.ts",
        "default": "./dist/*.js"
      }
    }
  },
  "author": "Artem Elkin",
  "repository": {
    "type": "git",
    "url": "https://github.com/artelk/async-context-tc39.git"
  },
  "license": "MIT",
  "keywords": [
    "async-context",
    "tc39",
    "AsyncContext",
    "AsyncLocal"
  ],
  "devDependencies": {
    "@types/node": "^22.13.14",
    "eslint": "^9.23.0",
    "eslint-plugin-async-context-tc39": "^1.0.2",
    "typescript-eslint": "^8.28.0",
    "vite": "^6.2.3",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "immutable": "^5.1.1"
  }
}
